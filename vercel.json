{
    "version": 2,
    
    "builds": [
        {
            "src": "/api/index.js",
            "use": "@vercel/node"
        }
    ],

    "routes": [
        {
            "src": "/api/auth(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/users(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/products(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/cart(.*)",
            "dest": "/api/index.js"
        }
    ]
}